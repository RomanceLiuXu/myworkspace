<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE configuration PUBLIC "-//ibatis.apache.org//DTD Config 3.0//EN" 
	"http://ibatis.apache.org/dtd/ibatis-3-config.dtd">
<configuration>
<!-- 全局设置 -->
   <settings >
   <setting name="logImpl" value="STDOUT_LOGGING"/>
   <!-- 开启二级缓存开关 -->
   <setting name="cacheEnabled" value="true"/>
   </settings>
  <typeAliases>
   	<typeAlias type="com.misu.bean.ProductType" alias="productType"/>
   	<typeAlias type="com.misu.bean.Product" alias="product"/>
   	<typeAlias type="com.misu.bean.Customer" alias="customer"/>
   	<typeAlias type="com.misu.bean.SysUser" alias="Sysuser"/>
   	<typeAlias type="com.misu.bean.Role" alias="role"/>
   	<typeAlias type="com.misu.common.ProductInfo" alias="productInfo"/>
   </typeAliases>
</configuration> 